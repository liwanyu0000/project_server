<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liwanyu.project_server.mapper.TestTableMapper">

    <select id="selectAll" resultType="top.liwanyu.project_server.model.TestTable">
        SELECT id,name FROM test_table;
    </select>

    <select id="selectByPrimaryKey" resultType="java.lang.String">
        SELECT name FROM test_table WHERE id = #{id};
    </select>

</mapper>
